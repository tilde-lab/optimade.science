{
  "name": "optimade.science",
  "private": true,
  "version": "1.1.0",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && rollup -c && npm run size",
    "dev": "rollup -c -w",
    "start": "sirv dist --single",
    "validate": "svelte-check",
    "size": "size-limit",
    "lint": "eslint src",
    "test": "jest src",
    "test:watch": "npm run test -- --watch",
    "clean": "rm -rf ./dist",
    "surge": "surge --project ./dist --domain optimade.surge.sh"
  },
  "engines": {
    "node": ">= 8"
  },
  "browserslist": [
    "extends browserslist-config-google"
  ],
  "size-limit": [
    {
      "limit": "130 KB",
      "path": "dist/index.html"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@size-limit/preset-app": "^4.6.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/svelte": "^3.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "@types/jest": "^26.0.15",
    "autoprefixer": "^10.0.1",
    "babel-jest": "^26.6.1",
    "browserslist-config-google": "latest",
    "core-js": "^3.6.5",
    "eslint": "^7.10.0",
    "eslint-plugin-svelte3": "^3.0.0",
    "git-scripts": "^0.4.3",
    "jest": "^26.6.1",
    "postcss": "^8.4.8",
    "postcss-easy-import": "^4.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-load-config": "^3.0.0",
    "postcss-preset-env": "^7.4.2",
    "postcss-url": "^10.1.3",
    "rollup": "^2.3.4",
    "rollup-plugin-bundle-html-plus": "^1.4.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^3.0.0",
    "rollup-plugin-import-resolver": "^1.0.4",
    "rollup-plugin-inline-svg": "^2.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-builtins": "2.0.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-visualizer": "^4.1.1",
    "sass": "^1.44.0",
    "size-limit": "^4.6.0",
    "surge": "^0.23.1",
    "svelte": "^3.31.0",
    "svelte-asyncable": "^2.0.4",
    "svelte-check": "^1.0.0",
    "svelte-code-editor": "^1.0.1",
    "svelte-content-loader": "^1.1.3",
    "svelte-jester": "^1.1.5",
    "svelte-json-tree-view": "^1.0.0",
    "svelte-morphing-modal": "^2.2.0",
